{
  "name": "aws-to-cwe",
  "version": "1.0.0",
  "description": "Generate Suspected CloudTrail Events from Terraform HCL",
  "main": "./lib/index.js",
  "bin": {
    "tf2cwe": "./bin/tf2cwe.js"
  },
  "scripts": {
    "test": "jest --runInBand --ci --no-cache --detectLeaks",
    "format": "prettier --write --config package.json lib",
    "format:check": "npm run format -- --check --no-write",
    "postinstall": "scripts/postinstall",
    "build": "webpack",
    "build:wasm": "tree-sitter generate --no-bindings && tree-sitter build-wasm",
    "playground": "tree-sitter playground",
    "watch:grammar": "scripts/watch"
  },
  "author": "Michael Neil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mneil/tf2cwe/issues"
  },
  "keywords": [
    "aws",
    "terraform",
    "cloudwatch",
    "event",
    "cloudtrail",
    "parser",
    "compiler"
  ],
  "repository": {
    "type": "git",
    "repository": "git+https://github.com/mneil/tf2cwe.git"
  },
  "devDependencies": {
    "@cdktf/provider-docker": "6.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "arg": "^5.0.2",
    "cdktf": "^0.15.4",
    "constructs": "^10.1.250",
    "copy-webpack-plugin": "^11.0.0",
    "jest": "^29.4.2",
    "nan": "^2.17.0",
    "prettier": "^2.8.4",
    "tree-sitter": "^0.20.1",
    "tree-sitter-cli": "0.20.6",
    "tree-sitter-go": "^0.19.1",
    "tree-sitter-hcl": "github:MichaHoffmann/tree-sitter-hcl#0ff887f2a60a147452d52db060de6b42f42f1441",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "web-tree-sitter": "^0.20.7",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "overrides": {
    "tree-sitter-cli": "0.20.6"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "ci": true,
    "automock": false,
    "bail": true,
    "maxConcurrency": 1,
    "maxWorkers": 1,
    "testMatch": [
      "**/*.test.ts"
    ],
    "testTimeout": 10000,
    "verbose": true,
    "detectLeaks": true,
    "errorOnDeprecated": true
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "trailingComma": "all",
    "semi": true,
    "singleQuote": false,
    "printWidth": 120,
    "overrides": [
      {
        "files": "*.ts",
        "options": {
          "parser": "typescript"
        }
      }
    ]
  }
}
